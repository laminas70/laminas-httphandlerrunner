{
    "name": "laminas70/laminas-httphandlerrunner",
    "description": "PHP 7.0 support for: Execute PSR-15 RequestHandlerInterface instances and emit responses they generate.",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "components",
        "mezzio",
        "psr-7",
        "psr-15",
        "PHP 7.0"
    ],
    "homepage": "https://laminas.dev",
    "support": {
        "docs": "https://docs.laminas.dev/laminas-httphandlerrunner/",
        "issues": "https://github.com/laminas/laminas-httphandlerrunner/issues",
        "source": "https://github.com/laminas/laminas-httphandlerrunner",
        "rss": "https://github.com/laminas/laminas-httphandlerrunner/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.0.0"
        }
    },
    "extra": {
        "laminas": {
            "config-provider": "Laminas\\HttpHandlerRunner\\ConfigProvider"
        }
    },
    "require": {
        "php": "^7.0.0",
        "psr/http-message": "^1.0",
        "psr/http-message-implementation": "^1.0",
        "psr/http-server-handler": "^1.0"
    },
    "require-dev": {
        "laminas/laminas-coding-standard": "*",
        "laminas/laminas-diactoros": "*",
        "phpunit/phpunit": "*",
        "vimeo/psalm": "*"
    },
    "autoload": {
        "psr-4": {
            "Laminas\\HttpHandlerRunner\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaminasTest\\HttpHandlerRunner\\": "test/"
        },
        "files": [
            "test/TestAsset/HeadersSent.php"
        ]
    },
    "scripts": {
        "check": [
            "@cs-check",
            "@test"
        ],
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --colors=always --coverage-clover clover.xml",
        "static-analysis": "psalm --shepherd --stats"
    },
    "replace": {
        "laminas/laminas-httphandlerrunner": "*"
    }
}
